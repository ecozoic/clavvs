    "postcss-syntax": "^0.31.0",
    "prettier": "^1.13.7",
    "pushstate-server": "^3.0.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
