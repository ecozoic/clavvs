  "description": "hypnotic alt pop // spectral trip hop",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "aws s3 sync ./dist s3://www.ecozoic.io",
    "serve": "cross-env NODE_ENV=production dotenv pushstate-server dist",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.jsx src",
    "lint:css": "stylelint './src/**/*.{js,jsx}'",
    "format": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/clavvs.git"
  },
  "author": "ecozoic <johnnn.mitchell@gmail.com>",
